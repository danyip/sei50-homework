
<section class="container">
    <div class="content-center">
        <div class="title"> <h1> New Student </h1> </div> 
        <%=  form_with model: @student, local: true do |f| %>
            <%= f.label :name %>
            <%= f.text_field :name %>
            <br>
            <%= f.label :email %>
            <%= f.text_field :email %>
            <br>
            <%= f.label :phone_number, "Phone Number" %>
            <%= f.text_field :phone_number %>
            <br>
            <%= f.label :address %>
            <%= f.text_field :address %>
            <br>
            <%= f.label :year %>
            <%= f.text_field :year %>
            <br>
            <%= f.submit %>
        <% end %>
     </div>

</section>
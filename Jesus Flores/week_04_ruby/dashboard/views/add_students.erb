 <section class="grid-container-1">
            <div class="grid-item-1">
                    <form action="/students/add" method="post">
                        <label for="first_name" >First Name</label>
                        <input id="first_name" name="first_name" type="text" >
                        <label for="second_name">Last Name</label>
                        <input id="second_name" name="second_name" type="text">

                        <label for="course_year" >Course Year</label>
                        <input id="course_year" name="course_year" type="text">

                        <label for="address">Address</label>
                        <input id="address" name="address" type="text">

                        <label for="email">Email</label>
                        <input id="email" name="email" type="text">

                        <label for="subject">Select Subject</label>
                       <select name="subject[]" id="subject[]" multiple="multiple" >
                                <option value="">Select Your Subject</option>
                                <% @subjects.each do |el| %>
                                    <option value="<%= el.id %>"> <%= el.name %> </option>
                                <% end %>
                       </select>
                        <button>Submit</button>
                    </form>
            </div>
</section>
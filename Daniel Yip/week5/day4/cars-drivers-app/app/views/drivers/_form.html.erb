<%= form_with model: @driver, local: true do |f| %>

  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br><br>
  
  <%= f.label :dob %>
  <br>
  <%= f.date_field :dob %>
  <br><br>
  
  <%= f.label :licenced %>
  <br>
  <%= f.check_box :licenced %>
  <br><br>
  
  <%= f.label :image %>
  <br>
  <%= f.text_field :image %>
  <br><br>

<%= f.submit %>

<%end%>
<%= form_with model: @pokemon, local: true do |f| %>

    <%= f.label :name  %>
    <br>
    <%= f.text_field :name %>
    <br><br>

    <%= f.label :level  %>
    <br>
    <%= f.text_field :level %>
    <br><br>

    <%= f.label :special_move  %>
    <br>
    <%= f.text_field :special_move %>
    <br><br>

    <%= f.label :image  %>
    <br>
    <%= f.text_field :image %>
    <br><br>

  <%= f.label :owner_id %>
  <br>
  <%= f.collection_select :owner_id, Owner.all, :id, :name %>
  <br><br>

<%= f.submit %>

<% end %>
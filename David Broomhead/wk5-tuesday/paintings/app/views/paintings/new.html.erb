<h2>Add New Painting</h2>

<%= form_with model: @painting, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <br>

  <%= f.label :price %>
  <%= f.text_field :price %>
  <br>

  <%= f.label :year %>
  <%= f.text_field :year %>
  <br>

  <%= f.label :url, 'Image URL' %>
  <%= f.text_field :url %>
  <br>

  <%= f.submit %>
<% end %>
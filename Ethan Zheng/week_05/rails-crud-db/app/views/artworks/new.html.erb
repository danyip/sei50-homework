<h2>Add New Artwork</h2>

<%= form_with model: @artwork, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :year %>
    <%= f.text_field :year %>
    <br>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
    <br>
    <%= f.label :image_url %>
    <%= f.text_field :image_url %>
    <br>
    <%= f.label :tags %> (Seperate tags with spaces)
    <%= f.text_field :tags %>
    <br>
    <%= f.submit %>


<% end %>
